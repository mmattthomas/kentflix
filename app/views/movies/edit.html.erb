<h1>Editing Movie</h1>

<%= render 'form', movie: @movie %>

<%= link_to 'Show', @movie, :class => 'btn btn-default' %> 
<%= link_to 'Back', movies_path, :class => 'btn btn-default' %>
<%= link_to 'DELETE', movie_path(@movie), :method => :delete, data: { confirm: "Sure?" }, :class => 'btn btn-danger' %>

